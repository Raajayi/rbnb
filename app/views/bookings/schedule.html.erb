
<%#= @band.bookings.count %>

<h1>Schedule</h1>
<!--h3><%#= @band.name %></h3-->
<% @band.bookings.each do |booking|%>
  <p>Booking Date: <%= booking.start_time.strftime("%d of %B, %Y") %></p>
  <p>Start Time: <%= booking.start_time.strftime("%I:%M %p") %></p>
  <p>End Time: <%= booking.end_time.strftime("%I:%M %p") %></p>
<% end %>
